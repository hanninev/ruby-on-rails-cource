<p id="notice"></p>

<h1>Sign in</h1>

<%= form_tag session_path do %>
  <p><label>username</label> <%= text_field_tag :username, params[:username], class: "form-control" %>
  <label>password</label> <%= password_field_tag :password, params[:password], class: "form-control" %></p>
  <p><%= submit_tag "Log in", class:"btn btn-primary" %></p>
<% end %>